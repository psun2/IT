var numbering = function (){
    i = 0;
    while(i < 10){
        document.write(i);
        i += 1;
    }   
}
numbering();

-------------------------------------------------------------------------------------------

function numbering (){
    i = 0;
    while(i < 10){
        document.write(i);
        i += 1;
    }   
}
numbering();




같은 결과값 발생

-------------------------------------------------------------------------------------------
익명함수
(function (){
    i = 0;
    while(i < 10){
        document.write(i);
        i += 1;
    }   
})(); 

